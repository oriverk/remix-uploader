{
  "hosting": {
    "site": "uploader-x193",
    "public": "./build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "functions": {
    "engines": {
      "node": "18"
    },
    "source": "/packages/functions",
    "region": "asia-northeast1"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  }
}
